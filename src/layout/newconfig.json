            "windowLayoutStyles": null,
            "runners": [
                {
                    "name": "gear1",
                    "type": "exec",
                    "entry": "gear -d gear1.ini -c gear1.css",
                    "runas": "window, default",
                    "class": "",
                    "dependencies": ["hibusd"],
                    "windowBoxStyles": "position: relative; left: 0%; top: 0%; width: 100%; height: 100%;"
                }]
        },
        {
            "id": "2",
